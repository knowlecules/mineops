{
  "name": "dhcpSettings",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "range": {
      "type": "array",
      "default": [
        "192.168.1.1",
        "192.168.1.253"
      ]
    },
    "netmask": {
      "type": "string",
      "default": "255.255.255.0"
    },
    "router": {
      "type": "array",
      "default": [
        "192.168.1.1"
      ]
    },
    "broadcast": {
      "type": "string",
      "default": [
        "192.168.1.255"
      ]
    },
    "server": {
      "type": "string",
      "default": "192.168.1.x"
    },
    "dns": {
      "type": "array",
      "default": [
        "8.8.8.8",
        "8.8.4.4"
      ]
    },
