{
  "name": "digger",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node --max-old-space-size=1024 .",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "posttest": "npm run lint && nsp check",
    "_precommit": "lint-staged && npm test",
    "pretty": "prettier --write",
    "pack": "gulp javascript"
  },
  "lint-staged": {
    "*.js": [
      "npm run pretty",
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "npm run pretty",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80
  },
  "jest": {
    "moduleFileExtensions": [
      "json",
      "js"
    ],
    "testRegex": ".*\\.spec\\.js$",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "*/db.json"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "@aliatech/loopback-mongo-aggregate-mixin": "^1.1.3",
    "body-parser": "^1.18.2",
    "bull": "^3.13.0",
    "bull-arena": "^2.8.1",
    "compression": "^1.7.4",
    "console-ten": "0.0.5",
    "cookie-parser": "^1.4.4",
    "csv": "^6.0.5",
    "dhcp": "github:knowlecules/node-dhcp",
    "digest-fetch": "^1.2.1",
    "dotenv": "^5.0.0",
    "faker": "^4.1.0",
    "formdata-node": "^4.3.2",
    "get-ip-range": "^4.0.1",
    "helmet": "^3.21.2",
    "http-proxy": "^1.18.0",
    "ip-subnet-calculator": "^1.1.8",
    "iprange": "^0.1.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^5.0.0",
    "loopback-connector-mongodb": "^3.4.2",
    "multer": "^1.4.4",
    "node-fetch": "^2.6.7",
    "nodemailer": "^5.1.1",
    "portscanner": "^2.2.0",
    "promise-limit": "^2.6.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "serve-favicon": "^2.0.1",
    "simple-ssh": "^1.0.0",
    "slack": "^11.0.0",
    "strong-error-handler": "^3.4.0",
    "ws": "^5.0.0",
    "www-authenticate": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "eslint-config-prettier": "2.9.0",
    "fs-extra": "^9.0.0",
    "gulp": "^4.0.2",
    "gulp-header": "^2.0.9",
    "gulp-javascript-obfuscator": "^1.1.6",
    "husky": "0.14.3",
    "ignore": "^5.1.4",
    "javascript-obfuscator": "^0.28.2",
    "jest": "^22.1.4",
    "klaw-sync": "^6.0.0",
    "lint-staged": "6.0.1",
    "nodemon": "^1.18.4",
    "nsp": "^2.1.0",
    "prettier": "1.10.2",
    "rewire": "^4.0.1",
    "supertest": "^4.0.2",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git remote add origin https://intelnick@bitbucket.org/intelnick/digger.git"
  },
  "license": "UNLICENSED",
  "description": "digger"
}
