<div class="container">
  <form id="frmCreatePlaceHolder" name="frmCreatePlaceHolder" onsubmit="return createPlaceHolder()" method="post" action="/api/minerInfos/provisionUnresponsiveMiner">
    <fieldset>
      <legend>Create miner place holder</legend>
    </fieldset>
    <div class="form-group">
      <label for="minerName">Miner type:</label>
      <select name="minerName" id="minerName">
        <% Object.keys(minerTypeMap).forEach(function(minerName){ %>
          <option value="<%=minerName%>"><%=minerName%></option>
        <%})%>
      </select>
    </div>
    <div class="form-group">
      <label for="macAddress">Mac Address:</label>
      <input type="text" name="macAddress" id="macAddress" value="<%=macAddress%>" />
    </div>
    <div class="form-group">
      <label for="ipAddress">IP Address: </label>
      <input type="text" name="ipAddress" id="ipAddress" value="<%=ipAddress%>" />
    </div>
    <div class="form-group">
      <button id="btnUpdatePools" class="btn btn-primary">Close</button>
      <button id="btnUpdatePools" type='submit' class="btn btn-primary">Create</button>
    </div>
  </form>
  <div class='result'></div>
</div>
<script>
$("#frmCreatePlaceHolder").submit(function(e){
  e.preventDefault();
});
</script>
